{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "event",
    "description": "",
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "lang": {
      "type": "enumeration",
      "enum": [
        "fr",
        "en",
        "de",
        "nl"
      ],
      "default": "en"
    },
    "date": {
      "type": "date"
    },
    "address": {
      "type": "text"
    },
    "uuid": {
      "type": "string",
      "unique": true
    },
    "description": {
      "type": "text",
      "maxLength": 250
    },
    "newsletter": {
      "type": "boolean",
      "private": true
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "events"
    },
    "travels": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::travel.travel",
      "mappedBy": "event"
    },
    "passengers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::passenger.passenger",
      "mappedBy": "event"
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "enabled_modules": {
      "type": "customField",
      "options": [
        "caroster-plus"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "administrators": {
      "type": "string"
    },
    "creator": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
